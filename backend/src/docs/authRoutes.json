{
  "routes": {
    "/auth/register": {
      "method": "POST",
      "requestBody": {
        "email": "user@example.com",
        "password": "password123"
      },
      "successResponse": {
        "success": true,
        "message": "User registered successfully",
        "data": {
          "userId": "12345",
          "email": "user@example.com",
          "accessToken": "eyJhbG...",
          "refreshToken": "eyJhbG..."
        }
      },
      "errorResponse": {
        "success": false,
        "message": "Email already exists"
      }
    },

    "/auth/login": {
      "method": "POST",
      "requestBody": {
        "email": "user@example.com",
        "password": "password123"
      },
      "successResponse": {
        "success": true,
        "message": "Login successful",
        "data": {
          "userId": "12345",
          "email": "user@example.com",
          "accessToken": "eyJhbG...",
          "refreshToken": "eyJhbG..."
        }
      },
      "errorResponse": {
        "success": false,
        "message": "Invalid credentials"
      }
    },

    "/auth/google": {
      "method": "GET",
      "response": "Redirects to Google OAuth consent screen"
    },

    "/auth/google/callback": {
      "method": "GET",
      "successResponse": {
        "cookies": {
          "accessToken": "eyJhbG...",
          "refreshToken": "eyJhbG..."
        },
        "redirect": "Frontend URL"
      }
    },

    "/auth/validate": {
      "method": "POST",
      "successResponse": {
        "success": true,
        "message": "Token is valid",
        "data": {
          "userId": "12345",
          "email": "user@example.com"
        }
      },
      "errorResponse": {
        "success": false,
        "message": "Invalid or expired token"
      }
    },

    "/auth/logout": {
      "method": "POST",
      "requiresAuth": true,
      "successResponse": {
        "success": true,
        "message": "Logged out successfully"
      },
      "errorResponse": {
        "success": false,
        "message": "Logout failed"
      }
    }
  }
}
